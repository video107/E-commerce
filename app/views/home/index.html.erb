      <div class="row">

        <div class="col-xs-12 col-sm-10 col-md-8 col-sm-offset-1 col-md-offset-2 contact-info">
          <div id="alert-contact">
            <div class="alert alert-success" role="alert"><strong>Your message has been sent.</strong></div>
          </div>
          <%= form_for @contact, :html => {id: "contact-form", class: "myform"}, :url => contacts_path do |f| %>
            <div class="row clearfix">
              <div class="col-xs-12 col-sm-6 col-md-6">
                <div class="form-group">
                  <%= f.label :name, "姓名", class: "control-label" %>
                  <div class="controls">
                    <%= f.text_field :name, placeholder: "您的姓名", class: "form-control input-lg requiredField", :data => {:"error-empty" => "請輸入您的姓名"} %>
                  </div>
                </div>
              </div>
              <div class="col-xs-12 col-sm-6 col-md-6">
                <div class="form-group">
                  <%= f.label :email, "Email", class: "control-label" %>
                  <div class=" controls">
                    <%= f.email_field :email, placeholder: "您的Email", class: "form-control input-lg requiredField", :data => {:"error-invalid" => "錯誤的Email格式", :"error-empty" => "請輸入您的Email"} %>
                  </div>
                </div>
              </div>
            </div>
            <div class="form-group">
              <label class="control-label" for="contact-message">Message</label>
              <%= f.label :content, "Content", class: "control-label" %>
              <div class="controls">
                <%= f.text_area :content, placeholder: "您要告訴我們什麼?", class: "form-control input-lg requiredField form-resize", rows: "5", :data => {:"error-empty" => "請輸入訊息"} %>
              </div>
            </div>
            <p>
              <%= f.submit "qweqe" %>
              <%= f.submit "送出Feedback", class: "btn btn-store btn-block"%>
            </p>
            <!-- <input type="hidden" name="submitted" id="submitted3" value="true"> -->
          <% end %>
        </div>
      </div>
    </div>
  </section>

